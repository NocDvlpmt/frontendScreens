{
  "name": "cra",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "animated-number-react": "^0.1.1",
    "axios": "^0.21.0",
    "compressorjs": "^1.0.6",
    "concurrently": "^5.2.0",
    "crypto-js": "^4.0.0",
    "emotion": "^10.0.27",
    "env-cmd": "^10.1.0",
    "history": "^5.0.0",
    "jest-emotion": "^10.0.32",
    "jest-fetch-mock": "^3.0.3",
    "jquery": "^3.5.1",
    "moment": "^2.27.0",
    "promise-memoize": "^1.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^5.4.3",
    "react-jss": "^10.3.0",
    "react-redux": "^7.2.1",
    "react-responsive-carousel": "^3.2.11",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-spring": "^8.0.27",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.3.3"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"npm run set:qontract:alias && npm run start:mock-server\"",
    "start:sit": "env-cmd -f .env.sit react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "echo 'Need to setup test case env'",
    "lint": "eslint --ext .js,.jsx src",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "eslint --ext .js,.jsx src -o ./eslint-reports/report.html -f html",
    "report": "eslint -f compact file.js > results.txt",
    "eject": "react-scripts eject",
    "build:pre-prod": "env-cmd -f .env.pre-prod cross-env CI='' react-scripts build",
    "build:sit": "env-cmd -f .env.sit cross-env CI='' react-scripts build",
    "build:production": "env-cmd -f .env.production cross-env CI='' react-scripts build",
    "set:qontract:alias": "java -jar qontract/lib/qontract.jar",
    "start:mock-server": "node scripts/run-qontract.js",
    "start:dev-server": "react-scripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,css}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "html",
      "lcov",
      "cobertura"
    ],
    "snapshotSerializers": [
      "jest-emotion/serializer"
    ]
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "cross-env": "^7.0.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.2.5",
    "jest-emotion": "^10.0.32",
    "jest-extended": "^0.11.5",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.2.13",
    "react-test-renderer": "^16.13.1"
  }
}
